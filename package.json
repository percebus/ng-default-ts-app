{
  "name": "ng-default-ts-app",
  "version": "0.1.0",
  "scripts": {
    "npm:install:global:ci": "npm install --location=global @angular/cli",
    "npm:install:global:all": "npm install --location=global @angular/cli open-cli",
    "setup": "npm run npm:install:global:all",
    "setup:ci": "npm run npm:install:global:ci",
    "prettier": "prettier .",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "ng": "ng",
    "ng:lint": "npm run ng -- lint",
    "ng:serve": "npm run ng -- serve",
    "ng:build": "npm run ng -- build",
    "ng:build:watch": "npm run ng:build -- --watch --configuration development",
    "ng:test": "npm run ng -- test",
    "ng:test:ci": "npm run ng:test -- --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "npm run prettier:check && npm run ng:lint",
    "style": "npm run prettier:write",
    "poststyle": "npm run lint",
    "browser": "open-cli http://localhost:4200",
    "prebuild": "npm run lint",
    "dist:development": "npm run ng:build -- --configuration development",
    "dist:production": "npm run ng:build -- --configuration production",
    "dist": "npm run ng:build",
    "pretest": "npm run lint",
    "test": "npm run ng:test",
    "test:ci": "npm run ng:test:ci",
    "pretest:ci": "npm run lint",
    "prestart": "npm run browser",
    "start": "npm run ng:serve",
    "predev": "npm run style",
    "dev": "npm start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "angular-eslint": "^19.3.0",
    "eslint": "8.57.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "~5.7.2"
  }
}
